@model PIM.Models.Atendimento

@{
    ViewData["Title"] = "Detalhes do Atendimento";
}

<h2>Detalhes do Atendimento</h2>
<dl class="row">
    <dt class="col-sm-2">Chamado</dt>
    <dd class="col-sm-10">@Model.ChamadoId</dd>
    <dt class="col-sm-2">Técnico</dt>
    <dd class="col-sm-10">@Model.UsuarioTecnicoId</dd>
    <dt class="col-sm-2">Data</dt>
    <dd class="col-sm-10">@Model.DataAtendimento.ToShortDateString()</dd>
    <dt class="col-sm-2">Ação</dt>
    <dd class="col-sm-10">@Model.AcaoRealizada</dd>
    <dt class="col-sm-2">Tempo Gasto</dt>
    <dd class="col-sm-10">@Model.TempoGasto</dd>
</dl>
<a asp-action="Edit" asp-route-id="@Model.AtendimentoId" class="btn btn-warning">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Voltar</a>