@model PIM.Models.Prioridade

@{
    ViewData["Title"] = "Excluir Prioridade";
}

<h2>Excluir Prioridade</h2>
<h3>Tem certeza que deseja excluir esta prioridade?</h3>
<dl class="row">
    <dt class="col-sm-2">Nome</dt>
    <dd class="col-sm-10">@Model.NomePrioridade</dd>
</dl>
<form asp-action="Delete">
    <input type="hidden" asp-for="PrioridadeId" />
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>