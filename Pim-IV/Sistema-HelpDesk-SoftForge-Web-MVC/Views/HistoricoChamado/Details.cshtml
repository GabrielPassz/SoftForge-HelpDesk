@model PIM.Models.HistoricoChamado

@{
    ViewData["Title"] = "Detalhes do Histórico";
}

<h2>Detalhes do Histórico</h2>
<dl class="row">
    <dt class="col-sm-2">Chamado</dt>
    <dd class="col-sm-10">@Model.ChamadoId</dd>
    <dt class="col-sm-2">Usuário</dt>
    <dd class="col-sm-10">@Model.UsuarioId</dd>
    <dt class="col-sm-2">Ação</dt>
    <dd class="col-sm-10">@Model.Acao</dd>
    <dt class="col-sm-2">Descrição</dt>
    <dd class="col-sm-10">@Model.Descricao</dd>
    <dt class="col-sm-2">Data</dt>
    <dd class="col-sm-10">@Model.DataAcao.ToShortDateString()</dd>
</dl>
<a asp-action="Edit" asp-route-id="@Model.HistoricoId" class="btn btn-warning">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Voltar</a>