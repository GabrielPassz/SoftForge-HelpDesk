@model PIM.Models.IAAnalise

@{
    ViewData["Title"] = "Excluir Análise de IA";
}

<h2>Excluir Análise de IA</h2>

<h3>Tem certeza que deseja excluir esta análise?</h3>
<div>
    <h4>ID: @Model.IAId</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-3">Categoria Prevista</dt>
        <dd class="col-sm-9">@Model.CategoriaPrevista</dd>
        <dt class="col-sm-3">Confiança</dt>
        <dd class="col-sm-9">@Model.Confianca</dd>
    </dl>
</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="IAId" />
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>