@model PIM.Models.Chamado

@{
    ViewData["Title"] = "Excluir Chamado";
}

<h2>Excluir Chamado</h2>
<h3>Tem certeza que deseja excluir este chamado?</h3>
<dl class="row">
    <dt class="col-sm-2">Protocolo</dt>
    <dd class="col-sm-10">@Model.Protocolo</dd>
</dl>
<form asp-action="Delete">
    <input type="hidden" asp-for="ChamadoId" />
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>