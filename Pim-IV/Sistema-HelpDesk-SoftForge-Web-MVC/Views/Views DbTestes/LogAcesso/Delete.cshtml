@model PIM.Models.LogAcesso

@{
    ViewData["Title"] = "Excluir Log de Acesso";
}

<h2>Excluir Log de Acesso</h2>
<h3>Tem certeza que deseja excluir este log?</h3>
<dl class="row">
    <dt class="col-sm-2">ID</dt>
    <dd class="col-sm-10">@Model.LogId</dd>
    <dt class="col-sm-2">Usuário</dt>
    <dd class="col-sm-10">@Model.UsuarioId</dd>
</dl>
<form asp-action="Delete">
    <input type="hidden" asp-for="LogId" />
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>