@model PIM.Models.Atendimento

@{
    ViewData["Title"] = "Excluir Atendimento";
}

<h2>Excluir Atendimento</h2>
<h3>Tem certeza que deseja excluir este atendimento?</h3>
<dl class="row">
    <dt class="col-sm-2">Chamado</dt>
    <dd class="col-sm-10">@Model.ChamadoId</dd>
</dl>
<form asp-action="Delete">
    <input type="hidden" asp-for="AtendimentoId" />
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>