@model PIM.Models.SLA

@{
    ViewData["Title"] = "Detalhes do SLA";
}

<h2>Detalhes do SLA</h2>
<dl class="row">
    <dt class="col-sm-2">ID</dt>
    <dd class="col-sm-10">@Model.SLAId</dd>
    <dt class="col-sm-2">Nome</dt>
    <dd class="col-sm-10">@Model.NomeSLA</dd>
    <dt class="col-sm-2">Prioridade</dt>
    <dd class="col-sm-10">@Model.PrioridadeId</dd>
</dl>
<a asp-action="Edit" asp-route-id="@Model.SLAId" class="btn btn-warning">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Voltar</a>