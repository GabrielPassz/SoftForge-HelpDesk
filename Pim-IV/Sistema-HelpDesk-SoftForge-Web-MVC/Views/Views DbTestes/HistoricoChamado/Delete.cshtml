@model PIM.Models.HistoricoChamado

@{
    ViewData["Title"] = "Excluir Histórico";
}

<h2>Excluir Histórico</h2>
<h3>Tem certeza que deseja excluir este histórico?</h3>
<dl class="row">
    <dt class="col-sm-2">Ação</dt>
    <dd class="col-sm-10">@Model.Acao</dd>
</dl>
<form asp-action="Delete">
    <input type="hidden" asp-for="HistoricoId" />
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>