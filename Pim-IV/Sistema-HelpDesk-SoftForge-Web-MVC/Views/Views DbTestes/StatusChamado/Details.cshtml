@model PIM.Models.StatusChamado

@{
    ViewData["Title"] = "Detalhes do Status";
}

<h2>Detalhes do Status</h2>
<dl class="row">
    <dt class="col-sm-2">ID</dt>
    <dd class="col-sm-10">@Model.StatusId</dd>
    <dt class="col-sm-2">Nome</dt>
    <dd class="col-sm-10">@Model.NomeStatus</dd>
</dl>
<a asp-action="Edit" asp-route-id="@Model.StatusId" class="btn btn-warning">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Voltar</a>