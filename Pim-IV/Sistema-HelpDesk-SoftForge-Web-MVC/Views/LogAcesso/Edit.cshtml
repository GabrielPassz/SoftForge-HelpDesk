@model PIM.Models.LogAcesso

@{
    ViewData["Title"] = "Editar Log de Acesso";
}

<h2>Editar Log de Acesso</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="LogId" />

    <div class="form-group">
        <label asp-for="LogId"></label>
        <input asp-for="LogId" class="form-control" readonly />
    </div>

    <p class="text-muted">Nenhum campo editável definido no modelo. Salvar não alterará propriedades adicionais.</p>

    <button type="submit" class="btn btn-primary">Salvar</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}