@model IEnumerable<PIM_FINAL.Models.IaAnalise>

@{
    ViewData["Title"] = "Análises IA";
}

<h1>Análises IA</h1>
<table class="table">
    <thead>
  <tr>
  <th>Id</th>
       <th>Chamado</th>
       <th>Categoria Prevista</th>
   <th>Confiança</th>
   <th>Data</th>
   <th></th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
    <tr>
   <td>@item.IaId</td>
   <td>@item.ChamadoId</td>
  <td>@item.CategoriaPrevista</td>
  <td>@item.Confianca</td>
       <td>@item.DataAnalise</td>
   <td>
 <a asp-action="Details" asp-route-id="@item.IaId">Detalhes</a> |
       <a asp-action="Edit" asp-route-id="@item.IaId">Editar</a> |
     <a asp-action="Delete" asp-route-id="@item.IaId">Excluir</a>
  </td>
    </tr>
    }
    </tbody>
</table>
<a asp-action="Create" class="btn btn-primary">Nova Análise</a>