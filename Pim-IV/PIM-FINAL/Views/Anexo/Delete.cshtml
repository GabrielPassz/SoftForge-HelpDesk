@model PIM_FINAL.Models.Anexo

@{
 ViewData["Title"] = "Excluir Anexo";
}
<h1>Excluir</h1>
<div>
 <h4>Tem certeza que deseja excluir?</h4>
 <dl class="row">
 <dt class="col-sm-2">Arquivo</dt>
 <dd class="col-sm-10">@Model.NomeArquivo</dd>
 <dt class="col-sm-2">Caminho</dt>
 <dd class="col-sm-10">@Model.CaminhoArquivo</dd>
 </dl>
 <form asp-action="Delete" method="post">
 <input type="hidden" asp-for="AnexoId" />
 <button type="submit" class="btn btn-danger">Delete</button>
 <a asp-action="Index" class="btn btn-secondary">Cancel</a>
 </form>
</div>